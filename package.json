{
  "name": "smart-toc",
  "version": "0.0.1",
  "description": "an chrome extension that generates a table of content for webpage",
  "main": "src/js/index.js",
  "scripts": {
    "clean": "rm -rf dist/* smart-toc.zip",
    "start": "export ENV=development && npm run build:background && rollup --config rollup.config.js --watch",
    "build": "export ENV=production && npm run clean; npm run build:content && npm run build:background && npm run build:zip",
    "build:content": "rollup --config rollup.config.js",
    "build:background": "mkdir -p dist && cp -R src/background/* dist/",
    "build:zip": "zip -r smart-toc.zip dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FallenMax/smart-toc.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "table-of-content",
    "toc",
    "outline",
    "outliner"
  ],
  "author": "FallenMax@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FallenMax/smart-toc/issues"
  },
  "homepage": "https://github.com/FallenMax/smart-toc#readme",
  "devDependencies": {
    "@types/chrome": "^0.0.91",
    "@types/mithril": "^2.0.2",
    "rollup": "1.27.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.25.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "mithril": "^2.0.4",
    "tslib": "^1.10.0"
  }
}
